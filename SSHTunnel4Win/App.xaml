<Application x:Class="SSHTunnel4Win.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conv="clr-namespace:SSHTunnel4Win.Converters"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
        <conv:ConnectionStateToColorConverter x:Key="StateToColor"/>
        <conv:ConnectionStateToLabelConverter x:Key="StateToLabel"/>
        <conv:BoolToVisibilityConverter x:Key="BoolToVis"/>
        <conv:InverseBoolToVisibilityConverter x:Key="InvBoolToVis"/>
        <conv:InverseBoolConverter x:Key="InvBool"/>
        <conv:AuthMethodToBoolConverter x:Key="AuthMethodToBool"/>
        <conv:TunnelTypeToVisibilityConverter x:Key="TypeToVis"/>
        <conv:NullToVisibilityConverter x:Key="NullToVis"/>
    </Application.Resources>
</Application>
